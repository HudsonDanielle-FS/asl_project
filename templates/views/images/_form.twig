<form method="POST" action="/images/{% if image.id %}{{ image.id }}{% endif %}">

   
    <div>
        <label>Variant: </label>
        <select name="variantId">
            {% for variant in variants %}
            <option value="{{ variant.id }}">{{ variant.title }}</option>
            {% endfor %}
        </select>
    </div>
        <input type="submit" type="submit" />
    </div>
</form>